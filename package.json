{
  "name": "markdown-tables",
  "author": "Matt Jarrett",
  "version": "2.9.6",
  "description": "Helping the world move Excel table data into Markdown tables",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "ci": "npm run lint && npm run test",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "nyc ava",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "bin": {
    "markdown-tables": "./bin/markdown-tables"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/markdown-tables.git"
  },
  "keywords": [
    "excel",
    "md",
    "markdown",
    "github",
    "gitlab",
    "tables",
    "table"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cujarrett/markdown-tables/issues"
  },
  "homepage": "https://github.com/cujarrett/markdown-tables#readme",
  "dependencies": {
    "xlsx": "~0.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "~13.1.0",
    "@commitlint/config-conventional": "~13.1.0",
    "@semantic-release/changelog": "~5.0.1",
    "@semantic-release/commit-analyzer": "~8.0.1",
    "@semantic-release/exec": "~5.0.0",
    "@semantic-release/git": "~9.0.1",
    "@semantic-release/github": "~7.2.3",
    "@semantic-release/npm": "~7.1.3",
    "@semantic-release/release-notes-generator": "~9.0.3",
    "ava": "~3.15.0",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~7.32.0",
    "husky": "~7.0.2",
    "nyc": "~15.1.0",
    "semantic-release": "~17.4.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
