{
  "name": "markdown-tables",
  "version": "3.0.4",
  "license": "MIT",
  "author": "Matt Jarrett",
  "description": "Helping the world move Excel table data into Markdown tables",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "ci": "npm run lint && npm run test",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "c8 ava",
    "prepare": "husky install"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "bin": {
    "markdown-tables": "./bin/markdown-tables.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/markdown-tables.git"
  },
  "keywords": [
    "excel",
    "md",
    "markdown",
    "github",
    "gitlab",
    "tables",
    "table"
  ],
  "bugs": {
    "url": "https://github.com/cujarrett/markdown-tables/issues"
  },
  "homepage": "https://github.com/cujarrett/markdown-tables#readme",
  "dependencies": {
    "xlsx": "~0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "~17.4.2",
    "@commitlint/config-conventional": "~17.4.2",
    "@semantic-release/changelog": "~6.0.2",
    "@semantic-release/commit-analyzer": "~9.0.2",
    "@semantic-release/exec": "~6.0.3",
    "@semantic-release/git": "~10.0.1",
    "@semantic-release/github": "~8.0.7",
    "@semantic-release/npm": "~9.0.2",
    "@semantic-release/release-notes-generator": "~10.0.3",
    "ava": "~5.1.1",
    "c8": "~7.12.0",
    "commitizen": "~4.3.0",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~8.33.0",
    "husky": "~8.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
