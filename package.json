{
  "name": "markdown-tables",
  "author": "Matt Jarrett",
  "version": "2.7.0",
  "description": "Helping the world move Excel table data into Markdown tables",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "ci": "npm run lint && npm run test",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "nyc --reporter=lcov tape test/**/*.js | tap-spec && nyc report",
    "semantic-release": "semantic-release"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "bin": {
    "markdown-tables": "./bin/markdown-tables"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/markdown-tables.git"
  },
  "keywords": [
    "excel",
    "md",
    "markdown",
    "github",
    "gitlab",
    "tables",
    "table"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cujarrett/markdown-tables/issues"
  },
  "homepage": "https://github.com/cujarrett/markdown-tables#readme",
  "dependencies": {
    "xlsx": "~0.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~12.1.4",
    "@commitlint/config-conventional": "~12.1.4",
    "@semantic-release/changelog": "~5.0.1",
    "@semantic-release/commit-analyzer": "~8.0.1",
    "@semantic-release/exec": "~5.0.0",
    "@semantic-release/git": "~9.0.0",
    "@semantic-release/github": "~7.2.3",
    "@semantic-release/npm": "~7.1.3",
    "@semantic-release/release-notes-generator": "~9.0.2",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~7.27.0",
    "husky": "~4.3.8",
    "lodash": "~4.17.21",
    "nyc": "~15.1.0",
    "semantic-release": "~17.4.3",
    "tap-spec": "~2.2.2",
    "tape-async": "~2.3.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
