{
  "name": "markdown-tables",
  "version": "3.0.5",
  "license": "MIT",
  "author": "Matt Jarrett",
  "description": "Helping the world move Excel table data into Markdown tables",
  "type": "module",
  "exports": "./src/index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "ci": "npm run lint && npm run test",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "test": "c8 ava",
    "prepare": "husky install"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "bin": {
    "markdown-tables": "./bin/markdown-tables.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/markdown-tables.git"
  },
  "keywords": [
    "excel",
    "md",
    "markdown",
    "github",
    "gitlab",
    "tables",
    "table"
  ],
  "bugs": {
    "url": "https://github.com/cujarrett/markdown-tables/issues"
  },
  "homepage": "https://github.com/cujarrett/markdown-tables#readme",
  "dependencies": {
    "xlsx": "~0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.8.1",
    "@commitlint/config-conventional": "~19.8.1",
    "@semantic-release/changelog": "~6.0.3",
    "@semantic-release/commit-analyzer": "~13.0.1",
    "@semantic-release/exec": "~7.1.0",
    "@semantic-release/git": "~10.0.1",
    "@semantic-release/github": "~11.0.3",
    "@semantic-release/npm": "~12.0.1",
    "@semantic-release/release-notes-generator": "~14.0.3",
    "ava": "~6.3.0",
    "c8": "~10.1.3",
    "commitizen": "~4.3.1",
    "cz-conventional-changelog": "~3.3.0",
    "eslint": "~9.27.0",
    "husky": "~9.1.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
